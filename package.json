{
    "name": "eslint-plugin-ko-react",
    "version": "1.2.0",
    "description": "Eslint plugin for ko-react package",
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin"
    ],
    "author": "Retsam",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npx tsc --build tsconfig.json",
        "dist": "npx rollup -c",
        "prepare": "npm run dist",
        "run_tests": "mocha build/tests --recursive",
        "test": "npm run build && npm run run_tests"
    },
    "dependencies": {
        "@typescript-eslint/typescript-estree": "^1.12.0",
        "tsutils": "^3.14.0"
    },
    "devDependencies": {
        "@types/json-schema": "^7.0.3",
        "@types/node": "^12.6.8",
        "@typescript-eslint/experimental-utils": "^1.12.0",
        "@typescript-eslint/parser": "^1.12.0",
        "eslint": "~3.9.1",
        "husky": "^2.2.0",
        "mocha": "^3.1.2",
        "prettier": "1.17.1",
        "rollup": "^1.11.3",
        "rollup-plugin-typescript2": "^0.21.0",
        "typescript": "^3.4.5"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test"
        }
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "license": "ISC"
}
